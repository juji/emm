{
  "name": "emm",
  "version": "1.0.0",
  "description": "Advanced memory monitoring and analytics tools for ETL processes",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "etl-run": "./dist/bin/run.js",
    "etl-analyze": "./dist/bin/analyze.js",
    "etl-optimize": "./dist/bin/optimize.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "npm run test:npm",
    "test:npm": "npm run build && npx mocha ./dist/utils/logger.test.node.js",
    "test:node": "npm run build && node --test dist/**/*.test.js",
    "test:bun": "bun test src/",
    "test:deno": "deno test src/",
    "start": "bun run dist/bin/run.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "etl",
    "memory",
    "monitoring",
    "analytics",
    "visualization",
    "optimization"
  ],
  "author": "GitHub Copilot",
  "license": "MIT",
  "dependencies": {
    "archy": "^1.0.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "consola": "^3.4.2",
    "jq-in-the-browser": "^0.7.2",
    "ora": "^8.2.0"
  },
  "private": true,
  "devDependencies": {
    "@types/bun": "^1.2.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/sinon": "^17.0.4",
    "sinon": "^20.0.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  }
}
