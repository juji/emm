{
  "name": "emm",
  "version": "1.0.0",
  "description": "ETL Memory Monitor: Memory monitoring and printer for your js/ts script. Works on Node.js, Bun, Deno, and Cloudflare.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "emm": "./dist/bin/emm.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "echo 'Build deno and build npm!'",
    "dev": "tsc --watch",
    "test": "npm run build && node --test dist/**/*.test.js",
    "start": "bun run dist/bin/emm.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "memory",
    "monitoring",
    "etl",
    "typescript",
    "nodejs",
    "bun",
    "deno",
    "cloudflare"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "prettier": "^3.5.3",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "src/test.ts"
  ]
}